function equal(num){
    while (num!=user){
        var user = readInt("GUESS THE NUMBER!");
        if (num==user){
            break
        }
        if (num<user){
            println("Lower")
        }else{
            println("Higher");
        }
    }
    println("Congrats, you guessed the right number!");
}
