import random
import string
user=int(input("Enter a number: "))
lc =random.choice(string.ascii_lowercase)
uc = random.choice(string.ascii_uppercase)
num = random.choice(string.digits)
sc = random.choice(string.punctuation)
password=sc+lc+uc+num

print(''.join(random.choice(password) for _ in range(user)))


